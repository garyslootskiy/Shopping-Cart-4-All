{
	"manifest_version" : 2,
	"name" : "Shopping Cart 4 All",
	"version" : "0.001",
	"content_scripts" : [
		{
			"matches" : [
				"<all_urls>"
             ],
             "js" : ["content.js"]
        }
     ],
     "background" : {
                   "scripts" : ["background.js"]
     },
     "browser_action" : {
      "default_popup": "popup.html",
      "default_title": "Check to see if our code is on this page"
     }
}                  